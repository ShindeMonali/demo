<h1>Listing Users</h1>
<table>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>City</th>
    <th colspan="3"></th>    <th></th>
  </tr>
 
  <% @user.each do |user| %>
    <tr>
      <td><%= user.Firstname %></td>
      <td><%= user.Lastname %></td>
       <td><%= user.City %></td>
      <td><%= link_to 'Show', user_path(user) %></td>
      <td><%= link_to 'Edit', Edit_path(user) %></td>
      <td><%= link_to 'Destroy', user_path(user),
              method: :delete,
              data: { confirm: 'Are you sure?' } %></td>

    </tr>
  <% end %>
</table>
